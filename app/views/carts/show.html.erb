<p style="color: green"><%= notice %></p>

<%= turbo_stream_from "carts" %>
<%= turbo_frame_tag "carts" do %>
  <%= render @cart %>
<% end %>

<div class="btn-group" role="group" aria-label="Button group">
  <%= link_to "Back to carts", carts_path, class: 'btn btn-secondary' %>
  <%= button_to "Destroy this cart", @cart, method: :delete, class: 'btn btn-danger' %>
</div>